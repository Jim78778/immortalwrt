/dts-v1/;

#include "ipq8074.dtsi"
#include "ipq8074-ac-cpu.dtsi"
#include "ipq8074-ess.dtsi"
#include "ipq8074-nss.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	model = "Swaiot / Skyworth CPE-S10";
	compatible = "swaiot,cpe-s10", "qcom,ipq8074";

	aliases {
		serial0 = &blsp1_uart3;
		ethernet0 = &dp5; /* WAN - 2.5G */
		ethernet1 = &dp1; /* LAN1 - 1G */
		ethernet2 = &dp2; /* LAN2 - 1G */
	};

	chosen {
		stdout-path = "serial0:115200n8";
		bootargs-append = " swiotlb=1 coherent_pool=2M";
	};

	memory@40000000 {
		device_type = "memory";
		reg = <0x0 0x40000000 0x0 0x40000000>; /* 1GB */
	};

	keys {
		compatible = "gpio-keys";

		wps {
			label = "wps";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&tlmm 50 GPIO_ACTIVE_LOW>;
		};

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&tlmm 34 GPIO_ACTIVE_LOW>;
		};
	};

	leds {
		compatible = "gpio-leds";

		/* 4G indicators */
		led_4g_yellow {
			label = "4gyellow";
			gpios = <&tlmm 42 GPIO_ACTIVE_HIGH>;
		};

		led_4g_blue {
			label = "4gblue";
			gpios = <&tlmm 43 GPIO_ACTIVE_HIGH>;
		};

		/* System indicators */
		led_system_blue {
			label = "blue";
			gpios = <&tlmm 51 GPIO_ACTIVE_LOW>;
		};

		led_system_red {
			label = "red";
			gpios = <&tlmm 52 GPIO_ACTIVE_LOW>;
		};

		/* Ethernet indicators */
		led_eth_blue {
			label = "ethblue";
			gpios = <&tlmm 55 GPIO_ACTIVE_LOW>;
		};

		led_eth_yellow {
			label = "ethyellow";
			gpios = <&tlmm 56 GPIO_ACTIVE_LOW>;
		};

		/* 5G indicators */
		led_5g_blue {
			label = "5gblue";
			gpios = <&tlmm 63 GPIO_ACTIVE_HIGH>;
		};

		led_5g_yellow {
			label = "5gyellow";
			gpios = <&tlmm 64 GPIO_ACTIVE_HIGH>;
		};
	};
};

&tlmm {
	mdio_pins: mdio-pins {
		mdc {
			pins = "gpio68";
			function = "mdc";
			drive-strength = <8>;
			bias-pull-up;
		};

		mdio {
			pins = "gpio69";
			function = "mdio";
			drive-strength = <8>;
			bias-pull-up;
		};
	};
};

&blsp1_uart3 {
	status = "okay";
};

&cryptobam {
	status = "okay";
};

&crypto {
	status = "okay";
};

&prng {
	status = "okay";
};

&qpic_nand {
	status = "okay";

	nand@0 {
		reg = <0>;
		nand-ecc-strength = <4>;
		nand-ecc-step-size = <512>;
		nand-bus-width = <8>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			/* 0. rootfs - 250MB */
			partition@0 {
				label = "ubi";
				reg = <0x00000000 0x0fa00000>;
			};

			/* 1. 0:SBL1 - 320KB */
			partition@fa00000 {
				label = "0:SBL1";
				reg = <0x0fa00000 0x00050000>;
				read-only;
			};

			/* 2. 0:MIBIB - 64KB */
			partition@fa50000 {
				label = "0:MIBIB";
				reg = <0x0fa50000 0x00010000>;
				read-only;
			};

			/* 3. 0:BOOTCONFIG - 128KB */
			partition@fa60000 {
				label = "0:BOOTCONFIG";
				reg = <0x0fa60000 0x00020000>;
				read-only;
			};

			/* 4. 0:BOOTCONFIG1 - 128KB */
			partition@fa80000 {
				label = "0:BOOTCONFIG1";
				reg = <0x0fa80000 0x00020000>;
				read-only;
			};

			/* 5. 0:QSEE - 1.5MB */
			partition@faa0000 {
				label = "0:QSEE";
				reg = <0x0faa0000 0x00180000>;
				read-only;
			};

			/* 6. 0:QSEE_1 - 1.5MB */
			partition@fc20000 {
				label = "0:QSEE_1";
				reg = <0x0fc20000 0x00180000>;
				read-only;
			};

			/* 7. 0:DEVCFG - 64KB */
			partition@fda0000 {
				label = "0:DEVCFG";
				reg = <0x0fda0000 0x00010000>;
				read-only;
			};

			/* 8. 0:DEVCFG_1 - 64KB */
			partition@fdb0000 {
				label = "0:DEVCFG_1";
				reg = <0x0fdb0000 0x00010000>;
				read-only;
			};

			/* 9. 0:APDP - 64KB */
			partition@fdc0000 {
				label = "0:APDP";
				reg = <0x0fdc0000 0x00010000>;
				read-only;
			};

			/* 10. 0:APDP_1 - 64KB */
			partition@fdd0000 {
				label = "0:APDP_1";
				reg = <0x0fdd0000 0x00010000>;
				read-only;
			};

			/* 11. 0:RPM - 256KB */
			partition@fde0000 {
				label = "0:RPM";
				reg = <0x0fde0000 0x00040000>;
				read-only;
			};

			/* 12. 0:RPM_1 - 256KB */
			partition@fe20000 {
				label = "0:RPM_1";
				reg = <0x0fe20000 0x00040000>;
				read-only;
			};

			/* 13. 0:CDT - 64KB */
			partition@fe60000 {
				label = "0:CDT";
				reg = <0x0fe60000 0x00010000>;
				read-only;
			};

			/* 14. 0:CDT_1 - 64KB */
			partition@fe70000 {
				label = "0:CDT_1";
				reg = <0x0fe70000 0x00010000>;
				read-only;
			};

			/* 15. 0:APPSBLENV - 64KB */
			partition@fe80000 {
				label = "0:APPSBLENV";
				reg = <0x0fe80000 0x00010000>;
				read-only;
			};

			/* 16. 0:APPSBL - 640KB */
			partition@fe90000 {
				label = "0:APPSBL";
				reg = <0x0fe90000 0x000a0000>;
				read-only;
			};

			/* 17. 0:APPSBL_1 - 640KB */
			partition@ff30000 {
				label = "0:APPSBL_1";
				reg = <0x0ff30000 0x000a0000>;
				read-only;
			};

			/* 18. 0:ART - 256KB */
			partition@ffd0000 {
				label = "0:ART";
				reg = <0x0ffd0000 0x00040000>;
				read-only;
			};

			/* 19. 0:ETHPHYFW - 512KB */
			partition@10010000 {
				label = "0:ETHPHYFW";
				reg = <0x10010000 0x00080000>;
				read-only;
			};

			/* 20. ledeinfo - 64KB */
			partition@10090000 {
				label = "ledeinfo";
				reg = <0x10090000 0x00010000>;
			};

			/* 21. reserve - 64KB */
			partition@100a0000 {
				label = "reserve";
				reg = <0x100a0000 0x00010000>;
			};
		};
	};
};

&qusb_phy_0 {
	status = "okay";
};

&qusb_phy_1 {
	status = "okay";
};

&ssphy_0 {
	status = "okay";
};

&ssphy_1 {
	status = "okay";
};

&usb_0 {
	status = "okay";
};

&usb_1 {
	status = "okay";
};

&mdio {
	status = "okay";

	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";
	reset-gpios = <&tlmm 37 GPIO_ACTIVE_LOW>;

	ethernet-phy-package@0 {
		compatible = "qcom,qca8075-package";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0>;

		qcom,package-mode = "qsgmii";

		/* 1G PHYs for LAN ports */
		qca8075_1: ethernet-phy@0 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <0>;
		};

		qca8075_2: ethernet-phy@1 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <1>;
		};

		/* 2.5G PHY for WAN port */
		qca8081: ethernet-phy@24 {
			compatible = "ethernet-phy-id004d.d101";
			reg = <24>;
			reset-deassert-us = <10000>;
			reset-gpios = <&tlmm 85 GPIO_ACTIVE_LOW>;

			leds {
				#address-cells = <1>;
				#size-cells = <0>;

				led@0 {
					reg = <0>;
					color = <LED_COLOR_ID_GREEN>;
					function = LED_FUNCTION_WAN;
					default-state = "keep";
				};
			};
		};
	};
};

&switch {
	status = "okay";

	switch_lan_bmp = <(ESS_PORT1 | ESS_PORT2)>; /* LAN port bitmap */
	switch_wan_bmp = <ESS_PORT5>; /* WAN port bitmap */
	switch_mac_mode = <MAC_MODE_QSGMII>; /* mac mode for uniphy instance0 */
	switch_mac_mode1 = <MAC_MODE_SGMII_PLUS>; /* mac mode for uniphy instance1 */

	qcom,port_phyinfo {
		port@1 {
			port_id = <1>;
			phy_address = <0>;
		};
		port@2 {
			port_id = <2>;
			phy_address = <1>;
		};
		port@5 {
			port_id = <5>;
			phy_address = <24>;
			port_mac_sel = "QGMAC_PORT";
		};
	};
};

&edma {
	status = "okay";
};

/* LAN1 - 1G */
&dp1 {
	status = "okay";
	phy-mode = "qsgmii";
	phy-handle = <&qca8075_1>;
	label = "lan1";
};

/* LAN2 - 1G */
&dp2 {
	status = "okay";
	phy-mode = "qsgmii";
	phy-handle = <&qca8075_2>;
	label = "lan2";
};

/* WAN - 2.5G */
&dp5 {
	status = "okay";
	phy-mode = "sgmii";
	phy-handle = <&qca8081>;
	label = "wan";
};

&pcie_qmp0 {
	status = "okay";
};

&pcie0 {
	status = "okay";
	// Likely for WiFi
};

&pcie1 {
	status = "okay";
	// Likely for 5G module
};

&wifi {
	status = "okay";
	qcom,ath11k-calibration-variant = "cpe_s10_sky";
};
